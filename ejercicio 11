#include <iostream>
using namespace std;
int main(){
        int N;
        int C = 1;
        int A;
        cout << "Ingrese numero: ";
        cin >> N;
        for (A = 1; A <= N; A++) {
            C = C * A;
        }
        cout << "El factorial de numero " << N << " es " << C << std::endl;
    }
