#include <iostream>
#include <limits>

using namespace std;
int main()
{
        int n, u0 = 1, u1 = 1, u=05;

          cout << "Ingrese variable entera: ";
          cin >> n;

        if (n == 0) {
            u = u0;
        }
        else if (n == 1) {
            u = u1;
        }
        else {
            for (int i = 2; i <= n; i++) {
                u = u0 + u1;
                u0 = u1;
                u1 = u;
            }
        }

        cout << "El valor es "<< "u(" << n << ") = " << u << endl;
    }
 
