#include <iostream>
#include <cmath>
using namespace std;
using std::pow;
int main()
{
        double x1 = 0, x2=0, y1=0, y2=0, d=0;
            cout << "Introducir Punto x1 ";
            cin >> x1;
            cout << "introducir y1 ";
            cin >> y1;
            cout << "introducir x2 ";
            cin >> x2;
            cout << "introducir y2 ";
            cin >> y2;
            d = sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));
            cout << "Distancia " << d << std::endl;
        }
